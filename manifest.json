{
  "manifest_version": 3,
  "name": "Cookie Cleaner with Whitelist",
  "version": "1.0",
  "description": "Clears site data for all sites except whitelisted domains.",
  "permissions": [
    "cookies",
    "storage",
    "browsingData",
    "tabs",
    "scripting",
    "downloads",
    "activeTab"
  ],
  "host_permissions": [ "<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
